import sys
import Fonctions

t = open(sys.argv[1])

liste = []

for ligne in t :
    f = ligne.split()
    liste.append(f)

Fonctions.CleanVertical(liste)
Fonctions.CleanHorizontal(liste)
Fonctions.OffsetChecker(liste)

Ethernet = []
IPv4 = []
TCP = []
HTTP = []
Fonctions.Extract(liste,Ethernet,IPv4,TCP,HTTP)

for i in Ethernet:
    print(i)
for i in IPv4:
    print(i)
for i in TCP:
    print(i)
for i in HTTP:
    print(i)

Fonctions.affichage(liste,Ethernet,IPv4,TCP,HTTP)






